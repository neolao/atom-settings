".editor":
  "cmd-k": "markdown-writer:insert-link"
  "cmd-i": "markdown-writer:toggle-italic-text"
  "cmd-b": "markdown-writer:toggle-bold-text"
  "cmd-'": "markdown-writer:toggle-code-text"
  "cmd-h": "markdown-writer:toggle-strikethrough-text"
  "alt-1": "markdown-writer:toggle-h1"
  "alt-2": "markdown-writer:toggle-h2"
  "alt-3": "markdown-writer:toggle-h3"
  "alt-4": "markdown-writer:toggle-h4"
  "alt-5": "markdown-writer:toggle-h5"
